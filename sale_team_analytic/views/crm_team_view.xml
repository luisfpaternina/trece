<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_view_id_inherit_crm_team" model="ir.ui.view">
        <field name="name">crm.team.view.form.inherit</field>
        <field name="model">crm.team</field>
        <field name="inherit_id" ref="sales_team.crm_team_view_form"/>
        <field name="priority">17</field>
        <field name="arch" type="xml">
          <xpath expr="//field[@name='invoiced_target']" position="after">
              <field name="analytic_account_id" options="{'no_create': True, 'no_create_edit':True}"/>
          </xpath>  
        </field>
    </record>
</odoo>
