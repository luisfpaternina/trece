<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_order_form_ol" model="ir.ui.view">
		    <field name="name">view_order_form_ol</field>
		    <field name="model">sale.order</field>
		    <field name="inherit_id" ref="sale.view_order_form"/>
		    <field name="arch" type="xml">
			    <xpath expr="//page[@name='order_lines']/field[@name='order_line']/tree[1]/field[@name='product_id']" position="after">
                    <field name="entrega_tienda" />
                </xpath>
		    </field>
	    </record>
    </data>
</odoo>
